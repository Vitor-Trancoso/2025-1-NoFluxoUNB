{
    "version": 2,
    "builds": [
        {
            "src": "pubspec.yaml",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build/web"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/:a*",
            "destination": "/"
        }
    ]
}